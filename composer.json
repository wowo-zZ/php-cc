{
  "name": "wowo-zz/php-cc",
  "description": "php code check tool",
  "type": "util",
  "authors": [
    {
      "name": "Gui Zheng",
      "email": "zhenggui5228@126.com"
    }
  ],
  "require": {
    "overtrue/phplint": "1.1.9",
    "squizlabs/php_codesniffer": "3.*",
    "symfony/console": "^3.2|^4.0|^5.0",
    "symfony/yaml": "^3.0|^4.0|^5.0"
  },
  "autoload": {
    "psr-4": {
      "wowozZ\\phpcc\\test\\": "test/",
      "wowozZ\\phpcc\\": "src/"
